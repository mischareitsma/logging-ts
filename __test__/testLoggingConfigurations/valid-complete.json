{
	"loggers": [
		{
			"name": "root",
			"handlers": [
				"handler1",
				"handler2"
			]
		},
		{
			"name": "app",
			"handlers": [
				"handler2"
			]
		}
	],
	"handlers": [
		{
			"name": "handler1",
			"type": "ConsoleHandler",
			"useColors": true,
			"logLevel": "TRACE"
		},
		{
			"name": "handler2",
			"type": "FileHandler",
			"logFile": "/var/log/some-file.log",
			"logLevel": "WARN"
		}
	]
}
