{
	"loggers": {
		"root": {
			"handlers": ["handler1", "handler2"]
		},
		"app": {
			"handlers": ["handler2"]
		}
	},
	"handlers": {
		"handler1": {
			"type": "ConsoleHandler",
			"useColors": true,
			"logLevel": "TRACE"
		},
		"handler2": {
			"type": "FileHandler",
			"logFile": "/var/log/some-file.log",
			"logLevel": "WARN"
		}
	}

}
